# CreteXtract Environment Variables
# Copy this file to .env and fill in your API keys

# PDF Conversion (Required for extraction)
DATALAB_API_KEY=your_datalab_api_key_here

# LLM Provider (choose one: openai, gemini, anthropic, deepseek)
LLM_PROVIDER=deepseek

# LLM API Keys (at least one required based on LLM_PROVIDER)
OPENAI_API_KEY=
GEMINI_API_KEY=
ANTHROPIC_API_KEY=
DEEPSEEK_API_KEY=
